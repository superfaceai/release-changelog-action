{
  "name": "release-changelog-action",
  "version": "1.0.0",
  "private": false,
  "description": "Release unreleased changes in your Keep a Changelog",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superfaceai/release-changelog-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "changelog",
    "release"
  ],
  "author": "Superface Team",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "keep-a-changelog": "^2.2.1"
  },
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/parser": "^5.58.0",
    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.33.0",
    "eslint-plugin-github": "^4.4.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.1",
    "jest-circus": "^29.4.1",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.4"
  }
}
